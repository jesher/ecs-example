{
    "executionRoleArn": "arn:aws:iam::720531668650:role/example-dev-ecs-role",
    "containerDefinitions": [
      {
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "example-dev-log-grp-api",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "example-dev"
          }
        },
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "cpu": 256,
        "environment": [
          { "name": "HASH_COMMIT", "value": "${HASH_COMMIT}" }
        ],
        "ulimits": [
          {
            "name": "nofile",
            "softLimit": 1000000,
            "hardLimit": 1000000
          }
        ],
        "memory": 256,
        "image": "image",
        "name": "api"
      }
    ],
    "memory": "512",
    "family": "api",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256"
  }
